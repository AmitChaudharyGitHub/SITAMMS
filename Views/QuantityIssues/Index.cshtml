        $(document).on('click', '#btnInsertBlock', function () {
            if ($('#txtBlockName').val() == '') {
                alert('Please enter block name');
                return false;
            }
            if ($('#prjtid1 option:selected').val() == '') {
                alert('Please select project');
                return false;
            }

            $.post('/QuantityIssues/AddNewBlock', { projectId: $('#prjtid1 option:selected').val(), blockName: $('#txtBlockName').val() }, function (response) {
                if (response.Status == 1) {
                    alert("Block added successfully");
                    GetBlocks();
                    $("#dialog-addBlock").dialog('close');
                }
                else if (response.Status == 2)
                    alert("Block not added");
                else
                    alert("Error occur");
            });

        });

        $(document).on('click','#btnInsertFloor',function () {
            if ($('#txtFloorName').val() == '') {
                alert('Please enter floor name');
                return false;
            }
            if ($('#prjtid1 option:selected').val() == '') {
                alert('Please select project');
                return false;
            }
            $.post('/QuantityIssues/AddNewFloor', { projectId: $('#prjtid1 option:selected').val(), floorName: $('#txtFloorName').val() }, function (response) {
                if (response.Status == 1) {
                    alert("Floor added successfully");
                    GetFloors();
                    $("#dialog-addFloor").dialog('close');
                }
                else if (response.Status == 2)
                    alert("Floor not added");
                else
                    alert("Error occur");
            });

        });

        $(document).on('click', '#btnInsertMachine', function () {
            if ($('#txtMachineName').val() == '') {
                alert('Please enter Machine name');
                return false;
            }
            if ($('#prjtid1 option:selected').val() == '') {
                alert('Please select project');
                return false;
            }
            $.post('/QuantityIssues/AddNewMachine', { projectId: $('#prjtid1 option:selected').val(), machineName: $('#txtMachineName').val() }, function (response) {
                if (response.Status == 1) {
                    alert("Machine added successfully");
                    GetMachines();
                    $("#dialog-addMachine").dialog('close');
                }
                else if (response.Status == 2)
                    alert("Machine not added");
                else
                    alert("Error occur");
            });

        });

    });

    function GetIssuedTo() {
        $.ajax({
            type: 'POST',
            url: '@Url.Action("Getprojectempblokflorcodes")', // Calling json method
            dataType: 'json',
            data: { id: $("#indent1").val() },
            // Get Selected Country ID.
            success: function (obj) {
                obj = $.parseJSON(obj)
                //  var r = toArray(obj);
                $("#prjtname1").val(obj.Pjtnames);
                $("#empsname1").val(obj.empnames);
                if (indentDate == '') {
                    $.get('@Url.Action("GetIndentDate")', { IndentNo: $("#indent1").val() }, function (response) {
                        if (response.IndentDate != "") {
                            $('#startdate').val(response.IndentDate);
                        }
                    });
                }
                else
                   $('#startdate').val(indentDate);
                //$("#sttsname1").val(obj.statuss);
                // $("#blocksssname1").val(obj.blockss);
                //$("#florssname1").val(obj.floors);
            },
            error: function (ex) {
                // alert('Failed to retrieve Project Code.' + ex);
            }
        });
        return false;
    }
</script>



