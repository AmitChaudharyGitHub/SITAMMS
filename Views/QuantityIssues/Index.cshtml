                // Get Selected Country ID.

                success: function (names) {

                    $.each(names, function (i, itname) {
                        $("#prjtid1").append('<option value="' + itname.Value + '">' +
                             itname.Text + '</option>');

                    });
                },

                error: function (ex) {
                    alert('Failed to retrieve Indent Number' + ex);
                }
            });
            return false;
        })


        var Indent = '@TempData["indent"]';
