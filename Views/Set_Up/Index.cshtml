@{
    ViewBag.Title = "Index";
    string layout = "";
    var cheking = Session["EmailCheck"].ToString();
    if (cheking == "admin@sitanet.in")
    {
        layout = "~/Views/Shared/_AdminLayout.cshtml";
    }
    else
    {
        layout = "~/Views/Shared/_Layout.cshtml";
    }

    Layout = layout;

}

<link href="@Url.Content("~/Content/themes/base/minified/jquery-ui.min.css")" rel="stylesheet" type="text/css" />


<style type="text/css">
    #dvLoading {
        background-color: transparent;
        background: #ffffff  url(../../Images/36.gif) no-repeat center center;
        height: 100px;
        width: 100px;
        position: fixed;
        z-index: 1000;
        left: 50%;
        top: 50%;
        margin: -25px 0 0 -25px;
    }
</style>




<script type="text/javascript">

    var GetGroup = '@Url.Action("GetItemGroupMaster", "IntraTransfer")';
    var GetItemByGroup = '@Url.Action("GetItemByGroup", "PurchaseRequisition")';
    var GetGrid = '@Url.Action("GetGetail", "Set_Up")';
    var UpdateGrid = '@Url.Action("SaveExcessItemWisePercentage", "Set_Up")';
</script>

<script src="~/Scripts/jquery-2.1.1.js"></script>


<section id="content_wrapper">
    <!-- Begin: Content -->
    <section id="content" class="table-layout animated fadeIn">

        <!-- begin: .tray-center -->
        <div class="tray tray-center">

            <div class="mw1000 center-block">
                <div class="admin-form theme-primary">

                    <div class="form-headerStyle theme-primary">
                        <div class="panel heading-border">
                            <div class="panel-body bg-light">
                                <div class="section-divider mb40" id="spy1">
                                    <span style="color:#4a89dc">Item Excess percentile Percentage</span>
                                </div>


                                <form class="row" id="loginForm">

                                    @using (Html.BeginForm())
                                    {
                                        @Html.AntiForgeryToken()

                                 

                                        <div class="form-horizontal">

                                            @Html.ValidationSummary(true, "", new { @class = "text-danger" })

                                            <div class="row">

                                                <div class="col-md-12">



                                                    <div class="col-md-4">
                                                        <div class="section">
                                                            <label class="field select">
                                                                @Html.DropDownList("ItemGroup", new SelectList(string.Empty, "Value", "Text"), "Select Item Group", htmlAttributes: new { @id = "ItemGroup", @class = "form-control" })
                                                                <i class="arrow"></i>
                                                            </label>
                                                        </div>
                                                    </div>


                                                    <div class="col-md-4">
                                                        <div class="section">
                                                            <label class="field select">
                                                                @Html.DropDownList("ItemMaster", new SelectList(string.Empty, "Value", "Text"), "Select Item", htmlAttributes: new { @id = "ItemMaster", @class = "form-control" })
                                                                <i class="arrow"></i>
                                                            </label>
                                                        </div>
                                                    </div>


                                                    <div class="col-md-3">
                                                        <input type="button" id="btn_Search" class="btn btn-primary" value="Search" />
                                                    </div>

                                                </div>
                                            </div>

                                            
                                            <br />

                                            <div class="row">
                                                <div class="col-sm-12">
                                                    <div id="dvLoading"></div>
                                                    @*<div id="#progress" style="display:none"><img src="~/Images/36.gif" /></div>*@
                                                    <div class="pager glyphicon-align-center" id="form_body"></div>
                                                </div>
                                            </div>
                                         
                                        </div>


                                        <br />

                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="col-md-2">
                                                    <input type="button" id="btnSubmit" class="btn btn-primary" value="Save" />
                                                </div>
                                            </div>
                                        </div>
                                     

                                    }



                                </form>


                            </div>
                        </div>


                    </div>

                    </div>
            </div>
        </div>
        <!-- end: .tray-center -->
        <!-- begin: .tray-right -->
        <!-- end: .tray-right -->

    </section>
    <!-- End: Content -->
</section>

<script src="~/Script/shetUp/ItemExcessPercentile.js"></script>