
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
    <!-- Font CSS (Via CDN) -->
    <link rel='stylesheet' type='text/css' href='http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700'>

    <!-- Theme CSS -->
    <link rel="stylesheet" type="text/css" href="~/Content/admindesigns.com/demos/admin/theme/assets/skin/default_skin/css/theme.css">

    <!-- Admin Forms CSS -->
    <link rel="stylesheet" type="text/css" href="~/Content/admindesigns.com/demos/admin/theme/assets/admin-tools/admin-forms/css/admin-forms.css">

    <!-- Favicon -->
    <link rel="shortcut icon" href="~/Content/admindesigns.com/demos/admin/theme/assets/img/favicon.ico">

    

</head>


<body class="external-page sb-l-c sb-r-c">
    <!-- Start: Main -->
    <div id="-main" class="animated fadeIn">

        <!-- Start: Content-Wrapper -->
        <section id="content_wrapper">
           
            <!-- begin canvas animation bg -->
            @*<div id="canvas-wrapper">
                <canvas id="demo-canvas"></canvas>
            </div>*@

  



            <!-- Begin: Content --> 
            <div>
                

                @RenderBody()
            </div>
            <!-- End: Content -->

        </section>
        <!-- End: Content-Wrapper -->

    </div>
    <!-- End: Main -->
    <!-- BEGIN: PAGE SCRIPTS -->
    <!-- jQuery -->
    <script src="~/Content/admindesigns.com/demos/admin/theme/vendor/jquery/jquery-1.11.1.min.js"></script>
    <script src="~/Content/admindesigns.com/demos/admin/theme/vendor/jquery/jquery_ui/jquery-ui.min.js"></script>

    <!-- CanvasBG Plugin(creates mousehover effect) -->
    <script src="~/Content/admindesigns.com/demos/admin/theme/vendor/plugins/canvasbg/canvasbg.js"></script>

    <!-- Theme Javascript -->
    <script src="~/Content/admindesigns.com/demos/admin/theme/assets/js/utility/utility.js"></script>
    <script src="~/Content/admindesigns.com/demos/admin/theme/assets/js/demo/demo.js"></script>
    <script src="~/Content/admindesigns.com/demos/admin/theme/assets/js/main.js"></script>

    <!-- Page Javascript -->
    <script type="text/javascript">
        jQuery(document).ready(function () {

            "use strict";

            // Init Theme Core
            Core.init();

            // Init Demo JS
            Demo.init();

            // Init CanvasBG and pass target starting location
            CanvasBG.init({
                Loc: {
                    x: window.innerWidth / 2,
                    y: window.innerHeight / 3.3
                },
            });

        });
    </script>
    <!-- END: PAGE SCRIPTS -->
</body>
</html>
